<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.SecondaryController">
    <Text text="Student Login" VBox.vgrow="ALWAYS" />
    <HBox alignment="CENTER" spacing="10.0">
        <Text text="Username:"/>
        <TextField text="Enter Username"/>
    </HBox>
    <HBox alignment="CENTER" spacing="10.0">
        <Text text="Password:"/>
        <TextField text="Enter Password"/>
    </HBox>
    <Button text="Login" onAction="#Login" minWidth="200" minHeight="50" maxWidth="200" maxHeight="50" VBox.vgrow="ALWAYS"/>
</VBox>
